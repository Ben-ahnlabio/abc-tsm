        "main.RootResponse": {
            "type": "object",
            "properties": {
                "build_type": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "time": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            }
        }
    }
}